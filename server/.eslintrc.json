{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "global-require": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "indent": "off",
    "no-restricted-globals": "off",
    "no-async-promise-executor": "off"
  }
}
